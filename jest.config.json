{
	"collectCoverageFrom": ["<rootDir>/src/**/*.js", "!<rootDir>/src/util/**/*.js"],
	"coverageDirectory": "<rootDir>/coverage/",
	"coverageThreshold": {
		"global": {
			"branches": 100,
			"functions": 100,
			"lines": 100,
			"statements": 100
		}
	},
	"collectCoverage": true,
	"testMatch": ["<rootDir>/__tests__/**/*.js"],
	"testEnvironment": "node",
	"transform": {
		"^.+\\.js$": "<rootDir>/node_modules/babel-jest"
	},
	"transformIgnorePatterns": ["<rootDir>/node_modules/(?!@constantiner)/"],
	"testPathIgnorePatterns": ["<rootDir>/node_modules/", "<rootDir>/__tests__/util/"]
}
